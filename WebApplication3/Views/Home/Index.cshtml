                                                    }

                                                    else {


                                                        var outputArray = []
                                                        var loop = 1

                                                        for (var loop = 1; loop <= 100; loop++) {
                                                            if (loop % FizzNum === 0 && loop % BuzzNum === 0) {
                                                                outputArray.push("FizzBuzz");
                                                            }
                                                            else
                                                                if (loop % FizzNum === 0) {
                                                                    outputArray.push("Fizz");
                                                                }
                                                                else
                                                                    if (loop % BuzzNum === 0) {
                                                                        outputArray.push("Buzz")
                                                                    }
                                                                    else {
                                                                        outputArray.push(loop);
                                                                    }
                                                        }

                                                        $("#FizzBuzzOut").text(outputArray.join(", "));

                                                    }
                                                });
                        </pre>
                            </div>
                        </div>
