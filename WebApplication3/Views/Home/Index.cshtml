                                                <div id=ShowCode class=collapse>
                                                    <br />
                                                    <br />
                                                    <br />
                                                    <p>$("#btnCalc").click(function ()<p />
                                                    <p>{<p />
                                                    <br />
                                                    <br />
                                                    <p>var num1 = $("#num1").val();<p />
                                                    <p>var num2 = $("#num2").val();<p />
                                                    <p>var num3 = $("#num3").val();<p />
                                                    <p>var num4 = $("#num4").val();<p />
                                                    <p>var num5 = $("#num5").val();<p />
                                                    <br />
                                                    <br />
                                                    <br />
                                                    <p>var sum = num1 + num2 + num3 + num4 + num5;<p />
                                                    <p>var product = num1 * num2 * num3 * num4 * num5;<p />
                                                    <p>var mean = sum / 5;<p />
                                                    <p>var min = Math.min(num1, num2, num3, num4, num5);<p />
                                                    <p>var max = Math.max(num1, num2, num3, num4, num5);<p />
                                                    <br />
                                                    <br />
                                                    <p>$("#output1").html("The <b>sum</b> of your numbers is <b>" + sum + "</b>");<p />
                                                    <p>$("#output2").html("The <b>product</b> of your numbers is <b>" + product + "</b>");<p />
                                                    <p>$("#output3").html("The <b>mean</b> of your numbers is <b>" + mean + "</b>");<p />
                                                    <p>$("#output4").html("The <b>minimum</b> number entered is <b>" + min + "</b>");<p />
                                                    <p>$("#output5").html("The <b>maximum</b> number entered is <b>" + max + "</b>");<p />
                                                    <p>});<p />
                                                    <br />
                                                    <p>});<p />

                                                </div>

